{
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/src"
    },
    {
      "name": "Python: Module",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}/src",
      "module": "sqlite_remote",
      "args": [
        "titan",
        "5000",
        "query_results.html",
        "select * from log_entry"
      ]
    },
  ]
}
